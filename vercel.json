{
  "version": 2,
  "functions": {
    "server/index.ts": {
      "runtime": "@vercel/node@20.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "client/dist"
}
